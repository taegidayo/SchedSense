{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "sounds": ["./local/assets/alarm.wav"]
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "name": "Schedsense",
    "slug": "Schedsense",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "27514e2f-8067-4e57-a268-eb6ac60a4890"
      }
    },
    "android": {
      "package": "com.MobPoj.SchedSense"
    },
    "assetBundlePatterns": ["**/*"]
  }
}
